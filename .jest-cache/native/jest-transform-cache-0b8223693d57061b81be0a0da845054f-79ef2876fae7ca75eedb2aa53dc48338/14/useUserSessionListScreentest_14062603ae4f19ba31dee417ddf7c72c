var _require = require('@testing-library/react-native'),
  renderHook = _require.renderHook,
  act = _require.act;
var useUserSessionListScreen = require("../../../../platform/screens/settings/UserSessionListScreen/useUserSessionListScreen").default;
var useUserSession = require("../../../../hooks").useUserSession;
describe('useUserSessionListScreen', function () {
  var mockList = jest.fn();
  var mockRevoke = jest.fn();
  var mockReset = jest.fn();
  beforeEach(function () {
    jest.clearAllMocks();
    useUserSession.mockReturnValue({
      list: mockList,
      revoke: mockRevoke,
      data: {
        items: [],
        pagination: {}
      },
      isLoading: false,
      errorCode: null,
      reset: mockReset
    });
  });
  it('returns items, handlers, and state', function () {
    var _renderHook = renderHook(function () {
        return useUserSessionListScreen();
      }),
      result = _renderHook.result;
    expect(result.current.items).toEqual([]);
    expect(typeof result.current.onRetry).toBe('function');
    expect(typeof result.current.onSessionPress).toBe('function');
    expect(typeof result.current.onRevoke).toBe('function');
  });
  it('calls list on mount', function () {
    renderHook(function () {
      return useUserSessionListScreen();
    });
    expect(mockReset).toHaveBeenCalled();
    expect(mockList).toHaveBeenCalledWith({
      page: 1,
      limit: 20
    });
  });
});
