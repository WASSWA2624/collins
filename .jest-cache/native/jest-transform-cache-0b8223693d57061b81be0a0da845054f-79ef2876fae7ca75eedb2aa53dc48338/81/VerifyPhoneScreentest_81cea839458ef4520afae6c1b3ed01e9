describe('VerifyPhoneScreen Component', function () {
  var mockT = jest.fn(function (key) {
    return key;
  });
  var baseHookReturn = {
    token: 'token',
    phone: '1234567890',
    isSubmitted: false,
    errorMessage: null,
    isLoading: false,
    isOffline: false,
    canSubmit: true,
    handleSubmit: jest.fn(),
    handleChangeToken: jest.fn(),
    handleChangePhone: jest.fn(),
    handleGoToLogin: jest.fn()
  };
  var platforms = [{
    name: 'Android',
    Component: VerifyPhoneScreenAndroid
  }, {
    name: 'iOS',
    Component: VerifyPhoneScreenIOS
  }, {
    name: 'Web',
    Component: VerifyPhoneScreenWeb
  }];
  beforeEach(function () {
    jest.clearAllMocks();
    useI18n.mockReturnValue({
      t: mockT
    });
    useVerifyPhoneScreen.mockReturnValue(baseHookReturn);
  });
  platforms.forEach(function (_ref4) {
    var name = _ref4.name,
      Component = _ref4.Component;
    it(`${name} renders form fields`, function () {
      var _renderWithTheme = renderWithTheme((0, _jsxRuntime.jsx)(Component, {})),
        getByTestId = _renderWithTheme.getByTestId;
      expect(getByTestId('verify-phone-screen')).toBeTruthy();
