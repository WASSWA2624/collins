      useVerifyPhoneScreen.mockReturnValue(Object.assign({}, baseHookReturn, {
        isOffline: true
      }));
      var _renderWithTheme2 = renderWithTheme((0, _jsxRuntime.jsx)(Component, {})),
        getByTestId = _renderWithTheme2.getByTestId;
