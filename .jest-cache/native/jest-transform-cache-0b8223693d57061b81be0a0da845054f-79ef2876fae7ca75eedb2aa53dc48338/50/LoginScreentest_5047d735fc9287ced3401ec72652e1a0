describe('LoginScreen Component', function () {
  var mockT = jest.fn(function (key) {
    return key;
  });
  var baseHookReturn = {
    email: 'user@example.com',
    password: 'password',
    tenantId: 'tenant',
    errorMessage: null,
    isLoading: false,
    isOffline: false,
    canSubmit: true,
    handleSubmit: jest.fn(),
    handleChangeEmail: jest.fn(),
    handleChangePassword: jest.fn(),
    handleChangeTenantId: jest.fn(),
    handleGoToRegister: jest.fn(),
    handleGoToForgotPassword: jest.fn()
  };
  beforeEach(function () {
    jest.clearAllMocks();
    useI18n.mockReturnValue({
      t: mockT
    });
    useLoginScreen.mockReturnValue(baseHookReturn);
  });
  describe('Android Implementation', function () {
    it('renders screen and form elements', function () {
      var _renderWithTheme = renderWithTheme((0, _jsxRuntime.jsx)(LoginScreenAndroid, {})),
        getByTestId = _renderWithTheme.getByTestId;
      expect(getByTestId('login-screen')).toBeTruthy();
      expect(getByTestId('login-email')).toBeTruthy();
      expect(getByTestId('login-password')).toBeTruthy();
      expect(getByTestId('login-tenant-id')).toBeTruthy();
      expect(getByTestId('login-button')).toBeTruthy();
    });
    it('renders offline state when offline', function () {
      useLoginScreen.mockReturnValue(Object.assign({}, baseHookReturn, {
        isOffline: true
      }));
      var _renderWithTheme2 = renderWithTheme((0, _jsxRuntime.jsx)(LoginScreenAndroid, {})),
        getByTestId = _renderWithTheme2.getByTestId;
      expect(getByTestId('login-offline-state')).toBeTruthy();
    });
  });
  describe('iOS Implementation', function () {
    it('renders screen and form elements', function () {
      var _renderWithTheme3 = renderWithTheme((0, _jsxRuntime.jsx)(LoginScreenIOS, {})),
        getByTestId = _renderWithTheme3.getByTestId;
      expect(getByTestId('login-screen')).toBeTruthy();
      expect(getByTestId('login-email')).toBeTruthy();
      expect(getByTestId('login-password')).toBeTruthy();
      expect(getByTestId('login-tenant-id')).toBeTruthy();
