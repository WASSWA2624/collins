var _jsxRuntime = require("react/jsx-runtime");
var React = require('react');
var _require = require('@testing-library/react-native'),
  render = _require.render;
var _require2 = require('styled-components/native'),
  ThemeProvider = _require2.ThemeProvider;
var _require3 = require("../../../../platform/components"),
  Text = _require3.Text;
var AuthFormLayoutAndroid = require("../../../../platform/components/layout/AuthFormLayout/AuthFormLayout.android").default;
var AuthFormLayoutIOS = require("../../../../platform/components/layout/AuthFormLayout/AuthFormLayout.ios").default;
var AuthFormLayoutWeb = require("../../../../platform/components/layout/AuthFormLayout/AuthFormLayout.web").default;
var useAuthFormLayout = require("../../../../platform/components/layout/AuthFormLayout/useAuthFormLayout").default;
var AuthFormLayoutIndex = require("../../../../platform/components/layout/AuthFormLayout/index.js");
var _require4 = require("../../../../platform/components/layout/AuthFormLayout/types.js"),
  SIZES = _require4.SIZES;
var lightThemeModule = require("../../../../theme/light.theme");
var lightTheme = lightThemeModule.default || lightThemeModule;
var renderWithTheme = function renderWithTheme(component) {
  return render((0, _jsxRuntime.jsx)(ThemeProvider, {
    theme: lightTheme,
    children: component
  }));
};
describe('AuthFormLayout Component', function () {
  var statusNode = (0, _jsxRuntime.jsx)(Text, {
    testID: "auth-status",
    children: "Status"
  });
  var actionNode = (0, _jsxRuntime.jsx)(Text, {
    testID: "auth-action",
    children: "Action"
  });
  var footerNode = (0, _jsxRuntime.jsx)(Text, {
    testID: "auth-footer",
    children: "Footer"
  });
  describe('Android Implementation', function () {
    it('renders full layout', function () {
      var _renderWithTheme = renderWithTheme((0, _jsxRuntime.jsx)(AuthFormLayoutAndroid, {
          title: "Title",
          description: "Description",
          status: statusNode,
          actions: actionNode,
          footer: footerNode,
          testID: "auth-layout",
          titleTestID: "auth-title",
          descriptionTestID: "auth-description",
          size: SIZES.MD
        })),
        getByTestId = _renderWithTheme.getByTestId;
      expect(getByTestId('auth-layout')).toBeTruthy();
      expect(getByTestId('auth-title')).toBeTruthy();
      expect(getByTestId('auth-description')).toBeTruthy();
      expect(getByTestId('auth-status')).toBeTruthy();
      expect(getByTestId('auth-action')).toBeTruthy();
      expect(getByTestId('auth-footer')).toBeTruthy();
    });
    it('renders minimal layout', function () {
      var _renderWithTheme2 = renderWithTheme((0, _jsxRuntime.jsx)(AuthFormLayoutAndroid, {
          title: "Title",
          testID: "auth-layout-min",
          titleTestID: "auth-title-min"
        })),
        getByTestId = _renderWithTheme2.getByTestId,
        queryByTestId = _renderWithTheme2.queryByTestId;
      expect(getByTestId('auth-layout-min')).toBeTruthy();
      expect(getByTestId('auth-title-min')).toBeTruthy();
      expect(queryByTestId('auth-description')).toBeNull();
    });
  });
  describe('iOS Implementation', function () {
    it('renders full layout', function () {
